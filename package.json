{
  "name": "bond-web-bun",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "bun:dev": "npx bun x --bun vite",
    "build": "tsc && vite build",
    "bun:build": "npx bun x --bun vite build",
    "lint": "npx biome lint ./src",
